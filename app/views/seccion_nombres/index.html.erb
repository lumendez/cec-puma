<p id="notice"><%= notice %></p>

<h1>Seccion Nombres</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seccion_nombres.each do |seccion_nombre| %>
      <tr>
        <td><%= seccion_nombre.nombre %></td>
        <td><%= link_to 'Show', seccion_nombre %></td>
        <td><%= link_to 'Edit', edit_seccion_nombre_path(seccion_nombre) %></td>
        <td><%= link_to 'Destroy', seccion_nombre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seccion Nombre', new_seccion_nombre_path %>
